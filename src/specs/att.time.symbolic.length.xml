<?xml version="1.0" encoding="UTF-8"?>
<tei:classSpec ident="att.time.symbolic.length" type="atts" module="mpm.core" mode="add" xmlns:tei="http://www.tei-c.org/ns/1.0">
    <tei:gloss>length attribute class</tei:gloss>
    
    <tei:desc>This attribute class provides attribute <tei:att>length</tei:att>.</tei:desc>
    
    <tei:attList>
        <tei:attDef ident="length" usage="req">
            <tei:gloss>beat length</tei:gloss>
            <tei:desc>Length indication in musical beats.</tei:desc>
            <tei:datatype>
                <tei:dataRef name="double">
                    <tei:dataFacet name="minExclusive" value="0.0"/>
                </tei:dataRef>
            </tei:datatype>
        </tei:attDef>
    </tei:attList>
</tei:classSpec>
