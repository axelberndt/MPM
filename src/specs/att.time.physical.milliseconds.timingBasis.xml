        <tei:attDef ident="milliseconds.timingBasis" usage="req">
            <tei:gloss>timing basis in milliseconds</tei:gloss>
            <tei:desc>defines the timing basis in milliseconds.</tei:desc>
            <tei:datatype>
                <tei:dataRef name="double">
                    <tei:dataFacet name="minExclusive" value="0.0"/>
                </tei:dataRef>
            </tei:datatype>
        </tei:attDef>
    </tei:attList>
</tei:classSpec>