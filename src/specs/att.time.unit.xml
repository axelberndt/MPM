<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="https://tei-c.org/release/xml/tei/custom/schema/relaxng/tei_odds.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<?xml-model href="https://tei-c.org/release/xml/tei/custom/schema/relaxng/tei_odds.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<tei:classSpec ident="att.time.unit" type="atts" module="mpm.core" mode="add" 
    xmlns:tei="http://www.tei-c.org/ns/1.0">
    <tei:gloss>time unit attribute class</tei:gloss>
    
    <tei:desc>This attribute class provides the attribute <tei:att>time.unit</tei:att>.</tei:desc>

    <tei:attList>
        <tei:attDef ident="time.unit" usage="opt">
            <tei:gloss>time unit</tei:gloss>
            <tei:desc>This attribute specifies the time unit to be either ticks (symbolic) or milliseconds (physical).</tei:desc>
            <tei:valList type="closed">
                <tei:valItem ident="ticks"/>
                <tei:valItem ident="milliseconds"/>
            </tei:valList>
        </tei:attDef>
    </tei:attList>
</tei:classSpec>
