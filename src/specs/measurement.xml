<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="https://tei-c.org/release/xml/tei/custom/schema/relaxng/tei_odds.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<?xml-model href="https://tei-c.org/release/xml/tei/custom/schema/relaxng/tei_odds.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<tei:elementSpec ident="measurement" module="mpm.core" mode="add" ns="http://www.cemfi.de/mpm/ns/1.0" xmlns:tei="http://www.tei-c.org/ns/1.0">
    <tei:gloss>measurement</tei:gloss>
    
    <tei:desc>This element represents a 64 bit floating-point list entry.</tei:desc>
    
    <tei:classes>
        <tei:memberOf key="att.value.double"/>
        <tei:memberOf key="att.id"/>
    </tei:classes>
    
    <tei:content>
        <tei:empty/>
    </tei:content>

    <tei:exemplum>
        <egXML xmlns="http://www.tei-c.org/ns/Examples">
            <measurement value="20.0"/>
        </egXML>
    </tei:exemplum>
    
    <tei:remarks>
        <tei:p>As child of <tei:gi>distribution.list</tei:gi> measurement represents one entry in the list.</tei:p>
    </tei:remarks>
</tei:elementSpec>
