<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="https://tei-c.org/release/xml/tei/custom/schema/relaxng/tei_odds.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<?xml-model href="https://tei-c.org/release/xml/tei/custom/schema/relaxng/tei_odds.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<tei:classSpec ident="att.intensity" type="atts" module="mpm.core" mode="add" xmlns:tei="http://www.tei-c.org/ns/1.0">
    <tei:gloss>loop attribute class</tei:gloss>
    
    <tei:desc>This attribute class provides attribute <tei:att>intensity</tei:att>.</tei:desc>
    
    <tei:attList>
        <tei:attDef ident="intensity" usage="opt">
            <tei:gloss>intensity</tei:gloss>
            <tei:desc>The intensity of the timing distortion effect.</tei:desc>
            <tei:datatype>
                <tei:dataRef name="double">
                    <tei:dataFacet name="minExclusive" value="0.0"/>
                </tei:dataRef>
            </tei:datatype>
            <tei:defaultVal>1.0</tei:defaultVal>
            <tei:remarks>
                <tei:p>This attribute specifies the type of the timing distortion.</tei:p>
            </tei:remarks>
        </tei:attDef>
    </tei:attList>
</tei:classSpec>
