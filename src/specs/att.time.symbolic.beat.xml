<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="https://tei-c.org/release/xml/tei/custom/schema/relaxng/tei_odds.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<?xml-model href="https://tei-c.org/release/xml/tei/custom/schema/relaxng/tei_odds.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<tei:classSpec ident="att.time.symbolic.beat" type="atts" module="mpm.core" mode="add" xmlns:tei="http://www.tei-c.org/ns/1.0">
    <tei:gloss>beat attribute class</tei:gloss>
    
    <tei:desc>This attribute class provides attribute <tei:att>beat</tei:att>.</tei:desc>
    
    <tei:attList>
        <tei:attDef ident="beat" usage="req">
            <tei:gloss>beat position</tei:gloss>
            <tei:desc>Position indication in musical beats.</tei:desc>
            <tei:datatype>
                <tei:dataRef name="double">
                    <tei:dataFacet name="minInclusive" value="1.0"/>
                </tei:dataRef>
            </tei:datatype>
        </tei:attDef>
    </tei:attList>
</tei:classSpec>
