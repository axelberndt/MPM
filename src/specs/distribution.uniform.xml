<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="https://tei-c.org/release/xml/tei/custom/schema/relaxng/tei_odds.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<?xml-model href="https://tei-c.org/release/xml/tei/custom/schema/relaxng/tei_odds.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<tei:elementSpec ident="distribution.uniform" module="mpm.core" mode="add" ns="http://www.cemfi.de/mpm/ns/1.0" xmlns:tei="http://www.tei-c.org/ns/1.0">
    <tei:gloss>uniform distribution</tei:gloss>
    
    <tei:desc>This defines a uniform distribution within the specified limits (inclusive).</tei:desc>
    
    <tei:classes>
        <tei:memberOf key="att.time.symbolic.date"/>
        <tei:memberOf key="att.distribution.limits"/>
        <tei:memberOf key="att.distribution.seed"/>
        <tei:memberOf key="att.id"/>
        <tei:memberOf key="model.distributionLike"/>
    </tei:classes>
    
    <tei:content>
        <tei:empty/>
    </tei:content>
    
    <tei:exemplum>
        <egXML xmlns="http://www.tei-c.org/ns/Examples">
            <distribution.uniform date="345.0" limit.lower="-10.0" limit.upper="35.6"/>
        </egXML>
    </tei:exemplum>
</tei:elementSpec>
